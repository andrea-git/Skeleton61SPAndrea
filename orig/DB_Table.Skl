; << ============ KeyField -- DB -- Check Key Field (CONTROL) ========= >>
; LK6B0385 (@!L_TABLEBUF!@ Not = @+_GRID_SYS_DATA+@)
@#DEF_IF#@ @+_GRID_SYS_ISKEYFIELD+@
   @#DEF_IF#@ @+_GRID_SYS_PICTURE+@
                 @!L_LEVEL!@ @+_OBJ_ATR_NAME+@-@+_GRID_SYS_ROW+@@+_GRID_SYS_COLUMN+@-VALUEBUF PIC @+_GRID_SYS_PICTURE+@.
   @#ELSE#@
                 @!L_LEVEL!@ @+_OBJ_ATR_NAME+@-@+_GRID_SYS_ROW+@@+_GRID_SYS_COLUMN+@-VALUEBUF.
   @#END_IF#@
@$CHECKKEYFIELD-@+_GRID_SYS_DATA+@$@
              IF @+_OBJ_ATR_NAME+@-@+_GRID_SYS_ROW+@@+_GRID_SYS_COLUMN+@-VALUEBUF Not = @!L_TABLEBUF!@
                 MOVE 1 TO TOTEM-IDX1
              END-IF
  @#DEF_SET#@ @!G_CHECKKEYFIELD!@ @"YES"@
@#END_IF#@
@$FDSL_BUFTOFLD_CELL$@
;           INQUIRE @+_OBJ_ATR_NAME+@(@+_GRID_SYS_ROW+@, @+_GRID_SYS_COLUMN+@)
;                   Record-DATA IN @!L_TABLEBUF!@
           MOVE @!L_TABLEBUF!@@!L_SUBSCRIPT!@ TO @+_GRID_SYS_DATA+@@!L_SUBSCRIPT!@
@$FDSL_FLDTOBUF_CELL$@
           MOVE @+_GRID_SYS_DATA+@@!L_SUBSCRIPT!@ TO @!L_TABLEBUF!@@!L_SUBSCRIPT!@
@$FDSL_BUFTOVALUEBUF_CELL$@
           MOVE @!L_TABLEBUF!@@!L_SUBSCRIPT!@ TO @+_OBJ_ATR_NAME+@-@+_GRID_SYS_ROW+@@+_GRID_SYS_COLUMN+@-VALUEBUF@!L_SUBSCRIPT!@
@$FDSL_GRIDTOBUF_CELL$@
           WHEN @+_GRID_SYS_ROW+@ ALSO @+_GRID_SYS_COLUMN+@
              INQUIRE @+_OBJ_ATR_NAME+@@!L_SUBSCRIPT!@(@+_GRID_SYS_ROW+@,@+_GRID_SYS_COLUMN+@)
                   CELL-DATA IN @!L_TABLEBUF!@@!L_SUBSCRIPT!@
@$FORM-TABLEDISPLAY$@
           MODIFY @!L_GRIDNAME!@@!L_SUBSCRIPT!@, X = @+_GRID_SYS_COLUMN+@, Y = @+_GRID_SYS_ROW+@,
                CELL-DATA = @!L_TABLEBUF!@@!L_SUBSCRIPT!@
       @#END_IF#@
