     @#END_IF#@
     @#GET_MEM#@ @"FORM-TABLEDISPLAY"@
	 @#CLEAR_MEM#@ @"FORM-TABLEDISPLAY"@
     @#DEF_IF#@ @+_OBJ_SYS_OCCURSTIME1+@
           END-PERFORM
     @#END_IF#@
     @#DEF_IF#@ @+_OBJ_SYS_OCCURSTIME2+@
           END-PERFORM
     @#END_IF#@           
