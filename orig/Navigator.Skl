; ===================== PRD -- Evaluate-Func. =======================
;
@$NAVIGATOR-EVALUATE$@
      * @+_OBJ_ATR_NAME+@ Data Navigator
   @#DEF_IF#@ @+_OBJ_ATR_VISIBLEFIRST+@
              WHEN @+_KEYSTATUS_SYS_ITEMNAME+@ = @+_OBJ_ATR_KEYFIRST+@
                 PERFORM @+_OBJ_ATR_NAME+@-First@!G_MODE!@
   @#END_IF#@
   @#DEF_IF#@ @+_OBJ_ATR_VISIBLENEXT+@
              WHEN @+_KEYSTATUS_SYS_ITEMNAME+@ = @+_OBJ_ATR_KEYNEXT+@
                 PERFORM @+_OBJ_ATR_NAME+@-Next@!G_MODE!@
   @#END_IF#@
   @#DEF_IF#@ @+_OBJ_ATR_VISIBLEPREVIOUS+@
              WHEN @+_KEYSTATUS_SYS_ITEMNAME+@ = @+_OBJ_ATR_KEYPREVIOUS+@
                 PERFORM @+_OBJ_ATR_NAME+@-Previous@!G_MODE!@
   @#END_IF#@
   @#DEF_IF#@ @+_OBJ_ATR_VISIBLELAST+@
              WHEN @+_KEYSTATUS_SYS_ITEMNAME+@ = @+_OBJ_ATR_KEYLAST+@
                 PERFORM @+_OBJ_ATR_NAME+@-Last@!G_MODE!@
   @#END_IF#@
   @#DEF_IF#@ @#EXP: @+_OBJ_ATR_VISIBLECURRENT+@ EQUAL @"BUTTONSTYLE"@ #@
              WHEN @+_KEYSTATUS_SYS_ITEMNAME+@ = @+_OBJ_ATR_KEYCURRENT+@
                 PERFORM @+_OBJ_ATR_NAME+@-Curr@!G_MODE!@
   @#END_IF#@
   @#DEF_IF#@ @+_OBJ_ATR_VISIBLEREFRESH+@
              WHEN @+_KEYSTATUS_SYS_ITEMNAME+@ = @+_OBJ_ATR_KEYREFRESH+@
                 PERFORM @+_OBJ_ATR_NAME+@-Refresh@!G_MODE!@
   @#END_IF#@
   @#DEF_IF#@ @+_OBJ_ATR_VISIBLEADD+@
              WHEN @+_KEYSTATUS_SYS_ITEMNAME+@ = @+_OBJ_ATR_KEYADD+@
                 PERFORM @+_OBJ_ATR_NAME+@-Add@!G_MODE!@
   @#END_IF#@
   @#DEF_IF#@ @+_OBJ_ATR_VISIBLEUPDATE+@
              WHEN @+_KEYSTATUS_SYS_ITEMNAME+@ = @+_OBJ_ATR_KEYUPDATE+@
                 PERFORM @+_OBJ_ATR_NAME+@-Update@!G_MODE!@
   @#END_IF#@
   @#DEF_IF#@ @+_OBJ_ATR_VISIBLEDELETE+@
              WHEN @+_KEYSTATUS_SYS_ITEMNAME+@ = @+_OBJ_ATR_KEYDELETE+@
                 PERFORM @+_OBJ_ATR_NAME+@-Delete@!G_MODE!@
   @#END_IF#@
   @#DEF_IF#@ @+_OBJ_ATR_VISIBLECLEAR+@
              WHEN @+_KEYSTATUS_SYS_ITEMNAME+@ = @+_OBJ_ATR_KEYCLEAR+@
       @#DEF_IF#@ @!G_FRAMENAME!@
                 PERFORM @!G_FRAMENAME!@-Clear@!G_MODE!@
       @#ELSE#@
                 PERFORM @!G_SCREENNAME!@-Clear@!G_MODE!@
       @#END_IF#@
   @#END_IF#@
;
@#DEF_IF#@ @#EXP: @+_OBJ_ATR_VISIBLECURRENT+@ AND
                  (NOT (@+_OBJ_ATR_VISIBLECURRENT+@ EQUAL @"BUTTONSTYLE"@)) #@
@$FORM-CLEAR$@
      * CLEAR COMBO/EDITOR OF NAVIGATOR
        @#DEF_IF#@ @+_OBJ_SYS_OCCURSTIME1+@
           PERFORM VARYING TOTEM-OCC1 FROM 1 BY 1 UNTIL TOTEM-OCC1 > @+_OBJ_SYS_OCCURSTIME1+@
        @#END_IF#@
        @#DEF_IF#@ @+_OBJ_SYS_OCCURSTIME2+@
           PERFORM VARYING TOTEM-OCC2 FROM 1 BY 1 UNTIL TOTEM-OCC2 > @+_OBJ_SYS_OCCURSTIME2+@
        @#END_IF#@
          @#DEF_SETPARA#@ @"1"@ @"TOTEM-OCC1"@
          @#DEF_SETPARA#@ @"2"@ @"TOTEM-OCC2"@
              MODIFY @+_OBJ_ATR_NAME+@-CURR@!G_MODE!@@+_OBJ_SYS_OCCURSSUBSCRIPT+@, VALUE SPACES
        @#DEF_IF#@ @+_OBJ_SYS_OCCURSTIME1+@
           END-PERFORM
        @#END_IF#@
        @#DEF_IF#@ @+_OBJ_SYS_OCCURSTIME2+@
           END-PERFORM
        @#END_IF#@
@#END_IF#@
