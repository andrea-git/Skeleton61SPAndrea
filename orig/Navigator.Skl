; ==================== EVT -- Navigator-Curr-Ev. ====================
;
@#DEF_IF#@ @#EXP: @+_OBJ_ATR_VISIBLECURRENT+@ AND
                  (NOT (@+_OBJ_ATR_VISIBLECURRENT+@ EQUAL @"BUTTONSTYLE"@)) #@
@<_PUSH_MEM>@
@<_SET_EXIST_DATASET>@
@<_SET_DATASET_MASTERFDSL>@
   ;
   ;
;
@$EVENT$@
@#DEF_SETANSI#@ @"EVENT"@
   @#DEF_IF#@ @!G_INTOOLBAR!@
       @+_OBJ_ATR_NAME+@-INTOOLBAR-GOTO@!G_MODE!@.
           EVALUATE Event-Type
      * set focus for toolbar
           WHEN CMD-GOTO
              ACCEPT @+_OBJ_ATR_NAME+@-Curr
           END-EVALUATE.

   @#END_IF#@
   ;
       @+_OBJ_ATR_NAME+@-Curr-Ev@!G_MODE!@.
   @#DEF_IF#@ @!G_INTOOLBAR!@
           PERFORM @+_OBJ_ATR_NAME+@-INTOOLBAR-GOTO@!G_MODE!@
   @#END_IF#@
   @#DEF_IF#@ @#EXP: @+_FDSL_SYS_SLREADTYPE+@ EQUAL @"DYNAMIC"@ OR
                     @+_FDSL_SYS_SLREADTYPE+@ EQUAL @"RANDOM"@ #@
   ; ===================================================================
   ; If the SlaveFD is Index/Relative, and AccessMode is "Dynamic".
   ; ===================================================================
           EVALUATE Event-Type
       @#DEF_IF#@  @#EXP: @+_OBJ_ATR_VISIBLECURRENT+@ EQUAL @"COMBOSTYLE"@ #@
           WHEN Ntf-Selchange
       @#ELSE_IF#@ @#EXP: @+_OBJ_ATR_VISIBLECURRENT+@ EQUAL @"EDITSTYLE"@  #@
           WHEN Msg-Validate
       @#END_IF#@
              PERFORM @+_OBJ_ATR_NAME+@-CURR-TOKEYIS@!G_MODE!@
              PERFORM @+_OBJ_ATR_NAME+@-Curr@!G_MODE!@
         @#DEF_IF#@ @!G_INTOOLBAR!@
              IF NOT @+_FDSL_SYS_FILESTATUS88+@
                 SET EVENT-ACTION TO EVENT-ACTION-CONTINUE
              END-IF
         @#END_IF#@
           END-EVALUATE.
   @#ELSE_IF#@ @#EXP: @+_FDSL_SYS_SLREADTYPE+@ EQUAL @"SEQUENTIAL"@ #@
   ; ===================================================================
   ; If the SlaveFD is Sequential or it's AccessMode is Sequential.
   ; And it's I/O Mode is "INPUT".
   ; ===================================================================
           EVALUATE Event-Type
       @#DEF_IF#@  @#EXP: @+_OBJ_ATR_VISIBLECURRENT+@ EQUAL @"COMBOSTYLE"@ #@
           WHEN Ntf-Selchange
       @#ELSE_IF#@ @#EXP: @+_OBJ_ATR_VISIBLECURRENT+@ EQUAL @"EDITSTYLE"@  #@
           WHEN Msg-Validate
       @#END_IF#@
              INQUIRE @+_OBJ_ATR_NAME+@-Curr@!G_MODE!@, VALUE @+_OBJ_ATR_NAME+@-CURR-TMP@!G_MODE!@
              PERFORM @+_OBJ_ATR_NAME+@-Curr@!G_MODE!@
         @#DEF_IF#@ @!G_INTOOLBAR!@
              IF NOT @+_FDSL_SYS_FILESTATUS88+@
                 SET EVENT-ACTION TO EVENT-ACTION-CONTINUE
              END-IF
         @#END_IF#@
           END-EVALUATE.
   @#END_IF#@
@<_POP_MEM>@

@#END_IF#@
@#DEF_SETANSI#@ @""@
