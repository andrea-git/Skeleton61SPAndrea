@$WORKING$@
   @#DEF_IF#@ @#EXP: (NOT (@!G_TEXTMODE!@)) AND (NOT (@!G_EXISTNAVIBMPHDL!@)) #@
      @#DEF_SET#@ @!G_EXISTNAVIBMPHDL!@ @"YES"@
      * DB-NAVIGATOR
       77 Navigator-Bmp     PIC S9(9) COMP-4.
   @#END_IF#@

      * NAVIGATOR (@+_OBJ_ATR_NAME+@)
       01 @+_OBJ_ATR_NAME+@-FUNCTION-FLAG@!G_MODE!@.
           05 @+_OBJ_ATR_NAME+@-EN-FIRST@!G_MODE!@     PIC 9(1) VALUE 1.
               88 @+_OBJ_ATR_NAME+@-BOOL-FIRST@!G_MODE!@   VALUE 1 FALSE 0.
           05 @+_OBJ_ATR_NAME+@-EN-PREV@!G_MODE!@      PIC 9(1) VALUE 1.
               88 @+_OBJ_ATR_NAME+@-BOOL-PREV@!G_MODE!@    VALUE 1 FALSE 0.
           05 @+_OBJ_ATR_NAME+@-EN-NEXT@!G_MODE!@      PIC 9(1) VALUE 1.
               88 @+_OBJ_ATR_NAME+@-BOOL-NEXT@!G_MODE!@    VALUE 1 FALSE 0.
           05 @+_OBJ_ATR_NAME+@-EN-LAST@!G_MODE!@      PIC 9(1) VALUE 1.
               88 @+_OBJ_ATR_NAME+@-BOOL-LAST@!G_MODE!@    VALUE 1 FALSE 0.
           05 @+_OBJ_ATR_NAME+@-EN-CURRBTN@!G_MODE!@   PIC 9(1) VALUE 1.
               88 @+_OBJ_ATR_NAME+@-BOOL-CURRBTN@!G_MODE!@ VALUE 1 FALSE 0.
           05 @+_OBJ_ATR_NAME+@-EN-REFRESH@!G_MODE!@   PIC 9(1) VALUE 1.
               88 @+_OBJ_ATR_NAME+@-BOOL-REFRESH@!G_MODE!@ VALUE 1 FALSE 0.
           05 @+_OBJ_ATR_NAME+@-EN-ADD@!G_MODE!@       PIC 9(1) VALUE 1.
               88 @+_OBJ_ATR_NAME+@-BOOL-ADD@!G_MODE!@     VALUE 1 FALSE 0.
           05 @+_OBJ_ATR_NAME+@-EN-UPDATE@!G_MODE!@    PIC 9(1) VALUE 1.
               88 @+_OBJ_ATR_NAME+@-BOOL-UPDATE@!G_MODE!@  VALUE 1 FALSE 0.
           05 @+_OBJ_ATR_NAME+@-EN-DELETE@!G_MODE!@    PIC 9(1) VALUE 1.
               88 @+_OBJ_ATR_NAME+@-BOOL-DELETE@!G_MODE!@  VALUE 1 FALSE 0.
           05 @+_OBJ_ATR_NAME+@-EN-CLEAR@!G_MODE!@     PIC 9(1) VALUE 1.
               88 @+_OBJ_ATR_NAME+@-BOOL-CLEAR@!G_MODE!@   VALUE 1 FALSE 0.
           05 @+_OBJ_ATR_NAME+@-EN-EXIT@!G_MODE!@      PIC 9(1) VALUE 1.
               88 @+_OBJ_ATR_NAME+@-BOOL-EXIT@!G_MODE!@    VALUE 1 FALSE 0.

@#DEF_IF#@ @#EXP: @+_FDSL_SYS_SLREADTYPE+@ EQUAL @"SEQUENTIAL"@ #@
       77 @+_OBJ_ATR_NAME+@-CURR-TMP@!G_MODE!@  PIC X(@+_FDSL_SYS_FDSIZE+@).

